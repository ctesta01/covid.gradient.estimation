@article{park_covid-19_2021,
	title = {{COVID}-19 {Deaths} in the {United} {States}: {Shifts} in {Hot} {Spots} over the {Three} {Phases} of the {Pandemic} and the {Spatiotemporally} {Varying} {Impact} of {Pandemic} {Vulnerability}},
