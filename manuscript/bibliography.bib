	month = aug,
	year = {2021},
	keywords = {COVID-19, Epidemiology, Ethnicity, Inequity, Race, Socioeconomic factor},
	pages = {101029},
	file = {ScienceDirect Full Text PDF:/Users/cht180/Zotero/storage/RXHCJ46Z/Lo et al. - 2021 - Race, ethnicity, community-level socioeconomic fac.pdf:application/pdf;ScienceDirect Snapshot:/Users/cht180/Zotero/storage/J2G7YBXG/S2589537021003096.html:text/html},
