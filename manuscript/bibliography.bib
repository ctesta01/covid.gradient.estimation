	keywords = {Basis penalty smooth, Differential equation smoothing, FELSPLINE, Finite window smoothing, Known boundary smoothing, Spline},
	pages = {931--955},
	file = {Accepted Version:/Users/cht180/Zotero/storage/NVBW57F7/Wood et al. - 2008 - Soap film smoothing.pdf:application/pdf;Snapshot:/Users/cht180/Zotero/storage/E4E6IW4B/j.1467-9868.2008.00665.html:text/html},
